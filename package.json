{
  "name": "alchemy-arcade",
  "version": "1.0.0",
  "license": "MIT",
  "scripts": {
    "start": "cross-env NODE_ENV=development parcel src/index.html -p 3000",
    "build": "cross-env NODE_ENV=production parcel build src/index.html --public-url ./",
    "e2e": "cypress open",
    "lint:js": "eslint src/**/**/*.js src/**/**/*.jsx tests/**/**/*.js",
    "lint:style": "stylelint src/**/**/*.css src/**/**/*.scss",
    "jest": "jest --watch"
  },
  "dependencies": {
    "babel-preset-react-app": "^3.1.2",
    "babel-runtime": "^6.26.0",
    "connected-react-router": "^4.3.0",
    "cross-env": "^5.2.0",
    "formik": "^1.0.1",
    "history": "^4.7.2",
    "node-sass": "^4.9.2",
    "parcel-bundler": "^1.9.7",
    "parcel-plugin-inlinesvg": "0.0.14",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-notification-system": "^0.2.17",
    "react-notification-system-redux": "^1.2.0",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "react-select": "^2.0.0",
    "react-svg-inline": "^2.1.1",
    "react-table": "^6.8.6",
    "redux": "^4.0.0",
    "redux-devtools": "^3.4.1",
    "redux-devtools-dock-monitor": "^1.1.3",
    "redux-devtools-log-monitor": "^1.4.0",
    "redux-logger": "^3.0.6",
    "redux-persist": "^5.10.0",
    "redux-thunk": "^2.3.0",
    "whatwg-fetch": "^2.0.4",
    "yup": "0.24.1"
  },
  "devDependencies": {
    "cypress": "^3.0.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.13.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.10.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^23.4.2",
    "prettier": "^1.13.7",
    "prettierrc": "0.0.0-5",
    "react-addons-test-utils": "^15.6.2",
    "react-test-renderer": "^16.4.2",
    "redux-mock-store": "^1.5.3",
    "stylelint": "^9.3.0",
    "stylelint-config-standard": "^18.2.0"
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,mjs}"
    ],
    "modulePathIgnorePatterns": [
      "tests"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less|scss|sass)$": "identity-obj-proxy"
    },
    "setupTestFrameworkScriptFile": "./tests/unit/setupTests.js",
    "testURL": "http://localhost:3000"
  }
}
